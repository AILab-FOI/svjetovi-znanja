{"id":2,"name":"EV002","note":"","pages":[{"conditions":{"actorId":1,"actorValid":false,"itemId":1,"itemValid":false,"selfSwitchCh":"A","selfSwitchValid":false,"switch1Id":1,"switch1Valid":false,"switch2Id":1,"switch2Valid":false,"variableId":1,"variableValid":false,"variableValue":0},"directionFix":false,"image":{"tileId":0,"characterName":"Evil","direction":4,"pattern":0,"characterIndex":7},"list":[{"code":355,"indent":0,"parameters":["(function(){const IME=\"DEDEK\";const OPIS=\"Ti si cool profesor koji priča s učenikom o programiranju u Pythonu. Učeniku se obraćaš sa ti, ne sa Vi. U pravilu davaj kratke odgovore do 3 rečenice osim ako priroda pitanja nije takva da je potreban duži odgovor.\";const UPIT=\"Molim te mi daj primjer for petlje.\";const MAX_LINE_LENGTH=51;function showMessagePreservingCode(text){const segments=parseCodeBlocks(text);let allLines=[];for(const seg of segments){if(seg.isCode){const codeLines=seg.content.split(\"\\n\");for(const codeLine of codeLines){const sublines=chunkLine(codeLine,MAX_LINE_LENGTH);allLines.push(...sublines)}}else{const wrappedLines=wrapText(seg.content,MAX_LINE_LENGTH);allLines.push(...wrappedLines)}}for(const line of allLines){$gameMessage.add(line)}}function chunkLine(line,limit){if(!line){return[\"\"]}const result=[];let startIdx=0;while(startIdx<line.length){result.push(line.slice(startIdx,startIdx+limit));startIdx+=limit}return result}function wrapText(text,limit){const words=text.trim().split(/\\s+/);const lines=[];let currentLine=\"\";for(const word of words){if(!currentLine){currentLine=word}else if(currentLine.length+1+word.length<=limit){currentLine+=\" \"+word}else{lines.push(currentLine);currentLine=word}}if(currentLine){lines.push(currentLine)}return lines}function parseCodeBlocks(text){const codeRegex=/```([\\s\\S]*?)```/g;let segments=[];let lastIndex=0;let match;while((match=codeRegex.exec(text))!==null){const index=match.index;const codeContent=match[1];if(index>lastIndex){segments.push({isCode:false,content:text.slice(lastIndex,index)})}segments.push({isCode:true,content:codeContent});lastIndex=codeRegex.lastIndex}if(lastIndex<text.length){segments.push({isCode:false,content:text.slice(lastIndex)})}return segments}foi_agent.ask(IME,\"Hello\").then(response=>{console.log(`[${ IME } exists] Test response:`,response);return foi_agent.ask(IME,UPIT)}).then(finalResponse=>{console.log(`${ IME } says:`,finalResponse);showMessagePreservingCode(finalResponse)}).catch(err=>{if(err.message.includes(\"No such agent\")){console.log(`[${ IME } does not exist] Creating a new agent...`);foi_agent.new(IME,OPIS).then(()=>{return foi_agent.ask(IME,UPIT)}).then(answer=>{console.log(`${ IME } says:`,answer);showMessagePreservingCode(answer)}).catch(finalErr=>{console.error(`Error after creating ${ IME }:`,finalErr)})}else{console.error(`Unexpected error while checking ${ IME }:`,err)}})})();"]},{"code":0,"indent":0,"parameters":[]}],"moveFrequency":3,"moveRoute":{"list":[{"code":0,"parameters":[]}],"repeat":true,"skippable":false,"wait":false},"moveSpeed":3,"moveType":0,"priorityType":1,"stepAnime":false,"through":false,"trigger":0,"walkAnime":true}],"x":12,"y":6},
